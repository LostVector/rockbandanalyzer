<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
    <component>
        <component-type>com.rkuo.WebApps.RockBandAnalyzerWeb.SongDetail</component-type>
        <component-class>com.rkuo.WebApps.RockBandAnalyzerWeb.SongDetailComponent</component-class>
    </component>
    <component>
        <component-type>com.rkuo.WebApps.RockBandAnalyzerWeb.SongTable</component-type>
        <component-class>com.rkuo.WebApps.RockBandAnalyzerWeb.SongTableComponent</component-class>
    </component>
    <component>
        <component-type>com.rkuo.WebApps.RockBandAnalyzerWeb.Components.LoginHeaderPanel</component-type>
        <component-class>com.rkuo.WebApps.RockBandAnalyzerWeb.LoginHeaderComponent</component-class>
    </component>
    <component>
        <component-type>com.rkuo.WebApps.RockBandAnalyzerWeb.NavigationHeader</component-type>
        <component-class>com.rkuo.WebApps.RockBandAnalyzerWeb.NavigationHeaderComponent</component-class>
    </component>
    <component>
        <component-type>com.rkuo.WebApps.RockBandAnalyzerWeb.Pages.FooterPanel</component-type>
        <component-class>com.rkuo.WebApps.RockBandAnalyzerWeb.FooterComponent</component-class>
    </component>
  <navigation-rule>
   <from-view-id>/submit.jsp</from-view-id>
    <navigation-case>
     <from-outcome>complete</from-outcome>
     <to-view-id>/samples/upload_complete.jsp</to-view-id>
   </navigation-case>
  </navigation-rule>
